{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "essential": true,
      "image": "public.ecr.aws/m1u5d7q3/hack/api:latest",
      "memory": 128,
      "name": "api"
    },
    {
      "essential": true,
      "image": "public.ecr.aws/m1u5d7q3/hack/frontend:latest",
      "memory": 128,
      "links": [
        "api"
      ],
      "name": "frontend",
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ]
    }
  ],
  "family": ""
}
